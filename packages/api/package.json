{
  "name": "web3.storage-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "(Apache-2.0 AND MIT)",
  "main": "dist/main.js",
  "scripts": {
    "start": "wrangler dev --env $(whoami)",
    "dev": "wrangler dev --env $(whoami)",
    "publish": "wrangler publish --env $(whoami)",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "test": "npm run test:e2e",
    "test:e2e": "npm-run-all -p -r test:e2e:**",
    "test:e2e:mock:cluster": "smoke -p 9094 test/mocks/cluster",
    "test:e2e:playwright": "playwright-test \"test/**/*.spec.js\" --sw src/index.js"
  },
  "devDependencies": {
    "@ipld/car": "3.1.3",
    "@nftstorage/ipfs-cluster": "^3.1.2",
    "@types/mocha": "^8.2.2",
    "assert": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "playwright-test": "^5.0.0",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "itty-router": "^2.3.10",
    "ipfs-car": "^0.3.5"
  }
}